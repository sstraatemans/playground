{
  "name": "@sstraatemans/sw_trpcclient",
  "description": "Shared tRPC types, Zod schemas, and client utilities",
  "author": {
    "name": "Steven Straatemans",
    "email": "steven@sstraatemans.nl",
    "url": "https://straatemans.nl"
  },
  "homepage": "https://suskeenwiske.dev/docs/trpc",
  "repository": {
    "type": "git",
    "url": "https://github.com/sstraatemans/playground.git"
  },
  "version": "0.2.1",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "ISC",
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./schemas": {
      "import": "./dist/generated/zod/schemas.js",
      "types": "./dist/generated/zod/schemas.d.ts"
    },
    "./client": {
      "import": "./dist/client/index.js",
      "types": "./dist/client/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --config eslint.config.js",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "prisma": {
    "seed": "tsx ./prisma/seed/seed.ts"
  },
  "dependencies": {
    "@sstraatemans/sw_trpcserver": "workspace:*"
  },
  "peerDependencies": {
    "superjson": "^2.2.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@config/eslint": "workspace:*",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@prisma/client": "^6.3.0",
    "@supabase/supabase-js": "^2.80.0",
    "@trpc/client": "^11.7.1",
    "@trpc/server": "^11.7.1",
    "@types/node": "^24.10.0",
    "cheerio": "^1.1.2",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.1",
    "openai": "^6.8.1",
    "pino": "^10.1.0",
    "prisma": "^6.3.0",
    "prisma-zod-generator": "^1.31.3",
    "superjson": "^2.2.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "zod": "^4.1.12"
  }
}
