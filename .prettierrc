{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^(react|react-native)$",
    "^(next|next/.*)$",
    "<THIRD_PARTY_MODULES>",
    "^@/",
    "^[./]",
    "^.*\\.types$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderTypeImportsSeparation": true,
  "importOrderMergeDuplicateImports": true,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "printWidth": 80,
  "tabWidth": 2
}
